{
  "name": "insta-launcher",
  "version": "1.0.0",
  "type": "commonjs",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "dev:google": "ts-node src/index.ts google",
    "build": "rimraf dist && tsc",
    "compile": "npm run build && rimraf out && npx caxa --input . --output out/insta-launcher.exe -- node dist/index.js"
  },
  "dependencies": {
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "playwright": "^1.30.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "caxa": "^3.0.1",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.0"
  }
}
